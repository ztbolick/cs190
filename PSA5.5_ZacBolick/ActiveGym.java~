public class 
 extends Gym () {
	super();
	private boolean basketballCourt;
	private boolean sauna;
	private boolean kidsClub;

	public ActiveGym() {
		basketballCourt = true;
		sauna = true;
		kidsClub = true;
	}

	public boolean getBasketballCourt () {
		return basketballCourt;
	}
	public boolean getSauna () {
		return sauna;
	}
	public boolean getKidsClub () {
		return kidsClub;
	}
	public setTowels (boolean sauna) {
		this.sauna = sauna;
	}
	public setPool (boolean basketballCourt) {
		this.basketballCourt = basketballCourt;
	}
	public setSquatRacks (boolean kidsClub) {
		this.kidsClub = kidsClub;
	}

}